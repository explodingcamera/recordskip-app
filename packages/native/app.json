{
  "expo": {
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "name": "RecordSkip",
    "slug": "recordskip",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-white.png",
    "splash": {
      "image": "./assets/images/logo-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
